        </owl:allValuesFrom>
        <owl:onProperty>
          <owl:ObjectProperty rdf:about="#hasPossibleValues"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
