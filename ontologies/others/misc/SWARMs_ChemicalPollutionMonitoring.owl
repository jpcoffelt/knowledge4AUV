          </owl:Restriction>
          <owl:Restriction>
            <owl:onProperty>
              <owl:FunctionalProperty rdf:ID="hasArgTerm"/>
            </owl:onProperty>
            <owl:someValuesFrom>
              <owl:Class rdf:ID="OVariable"/>
            </owl:someValuesFrom>
          </owl:Restriction>
          <owl:Restriction>
            <owl:allValuesFrom>
              <owl:Class rdf:ID="Node"/>
            </owl:allValuesFrom>
            <owl:onProperty>
              <owl:FunctionalProperty rdf:about="#isArgumentOf"/>
            </owl:onProperty>
          </owl:Restriction>
          <owl:Restriction>
            <owl:allValuesFrom>
              <owl:Class rdf:about="#OVariable"/>
            </owl:allValuesFrom>
            <owl:onProperty>
              <owl:FunctionalProperty rdf:about="#hasArgTerm"/>
            </owl:onProperty>
          </owl:Restriction>
