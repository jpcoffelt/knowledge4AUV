        <owl:onProperty>
          <owl:ObjectProperty rdf:about="#isNodeFrom"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:allValuesFrom rdf:resource="#ArgRelationship"/>
        <owl:onProperty>
          <owl:InverseFunctionalProperty rdf:ID="hasArgument"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:allValuesFrom rdf:resource="#Entity"/>
        <owl:onProperty>
          <owl:ObjectProperty rdf:about="#hasPossibleValues"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
