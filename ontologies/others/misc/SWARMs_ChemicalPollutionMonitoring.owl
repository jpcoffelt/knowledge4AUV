    <rdfs:subClassOf rdf:resource="#Node"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:allValuesFrom rdf:resource="#Entity"/>
        <owl:onProperty>
          <owl:ObjectProperty rdf:about="#hasPossibleValues"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <owl:disjointWith>
